uint8 tm_sec    # seconds (0-61, allowing for leap seconds)
uint8 tm_min    # minutes (0-59)
uint8 tm_hour   # hours (0-23)
uint8 tm_mday   # day of the month (1-31)
uint8 tm_mon    # month of the year (0-11)
uint8 tm_year   # years since 1900
uint8 tm_wday   # day of the week (Sunday is 0)
uint8 tm_yday   # day of the year (0-365)
uint8 tm_isdst  # daylight saving time flag (-1, 0, or 1)